SELECT MS.CODFIL, MS.TPNOTA, MS.NUMNOTA, MS.NUMCXA, MS.DTNOTA, MS.VLTOTAL, MS.TIPOPED, MS.DTEMISSAO, MS.DTPEDIDO, MS.DTCANCEL, MS.CONDPGTO, MS.NUMPEDVEN, MS.NOME, MS.CODCLI, MS.CODVENDR, MS.DTATUEST, MS.IDENTPROC, MS.*  *
FROM   MOV_SAIDA MS
WHERE  1 = 1
AND MS.CODFIL = &CODFIL
AND MS.NUMCXA = &PDV
AND MS.DTNOTA = TO_DATE(&DATA)
--AND MS.NUMNOTA = &NUMNOTA
--AND MS.VLTOTAL = &VALORTOTAL

ORDER BY VLTOTAL

