SELECT A.FILIAL,
	  A.NUMCUPOM,
	  A.NUMCXA,
	  A.VLTRANSACAO,
	  A.*
	  FROM TRANSACOES_TEF A
WHERE 
	 A.FILIAL = 34
	 AND A.NUMCXA = 13
	 AND A.NUMCUPOM = 142964
	 
	 

	 
SELECT  TEF, LANCTO, TEF-LANCTO DIV FROM (SELECT SUM(VLTRANSACAO) TEF 
	  FROM TRANSACOES_TEF A
WHERE 
	 A.FILIAL = 34
	 AND DTTRANSACAO = '20/10/2015'
	 AND STATUS <> 9) T1,

(SELECT LANCTO FROM (SELECT SUM(B.VALLAN) LANCTO
FROM CXA_LANCTO B
WHERE B.CODFIL = 34
AND B.DATENT = '20/10/2015'
AND B.CODEVE IN (6,7)
AND B.STATUS <> 9) T2);